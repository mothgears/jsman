{
  "presets": [
    ["@babel/env", {
      "targets": {
        "browsers": [
          "last 2 Chrome versions",
          "last 2 Firefox versions",
          "last 2 Edge versions",
          "last 2 Opera versions",
          "last 2 Safari versions"
        ],
        "node": "12"
      },
      "useBuiltIns": "usage",
      "corejs": { "version": 3, "proposals": true }
    }]
  ],
  "plugins": [
    "module:babel-plugin-add-module-exports"
  ]
}