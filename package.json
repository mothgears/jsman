{
  "name": "jsman",
  "version": "3.1.1",
  "description": "JavaScript Map alternative notation",
  "author": "Artem Varshavin",
  "license": "MIT",
  "keywords": [
    "map",
    "array",
    "notation"
  ],
  "browser": "lib.cjs",
  "main": "lib.es6",
  "exports": {
    "browser": {
      "import": "./lib.mjs",
      "require": "./lib.cjs"
    },
    "node": "./lib.es6"
  },
  "scripts": {
    "prepublishOnly": "babel lib.es6 -o lib.cjs | babel lib.es6 --config-file ./esm-babel.config.json -o lib.mjs",
    "build": "babel lib.es6 -o lib.cjs | babel lib.es6 --config-file ./esm-babel.config.json -o lib.mjs",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mothgears/jsman.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "babel-jest": "^29.0.2",
    "jest": "^29.0.2",
    "jest-environment-jsdom": "^29.0.2"
  },
  "dependencies": {
    "core-js": "^3.25.1"
  }
}
