{
  "name": "jsman",
  "version": "1.0.1",
  "description": "JavaScript Map alternative notation",
  "author": "Artem Varshavin",
  "license": "MIT",
  "keywords": [
    "map",
    "array",
    "notation"
  ],
  "main": "lib/index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=development node --experimental-modules --experimental-json-modules node_modules/jest/bin/jest.js",
    "build": "cross-env NODE_ENV=production babel src/index.mjs --out-dir lib",
    "build-test": "cross-env NODE_ENV=production jest"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mothgears/jsman.git"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "babel-plugin-add-module-exports": "^1.0.4",
    "cross-env": "^7.0.2",
    "jest": "^26.6.0",
    "jest-esm-transformer": "^1.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "mjs"
    ],
    "testMatch": [
      "**/tests/**/*.(js|mjs)"
    ],
    "transform": {
      ".mjs": "jest-esm-transformer"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "core-js": "^3.6.5"
  }
}
